<%= form_tag session_path id: 'login-form' do %>
  <fieldset>
  <%= label_tag :email %>
  <%= text_field_tag :email, nil, placeholder: 'your-email@secure.com' %>

  <%= label_tag :password %>
  <%= password_field_tag :password, nil, placeholder: 'Your Password'%>

  <%= submit_tag 'Login' %>
  </fieldset>
<% end %>
